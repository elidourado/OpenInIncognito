{
  "name": "Open In Incognito",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "Open the current URL in an incognito window.",
  "homepage_url": "http://misterburton.com/pages/open-in-incognito.html",
  "icons": {
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "38": "icons/icon76.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
        "19": "icons/icon19.png",
        "38": "icons/icon76.png"
    },
    "default_title": "Open In Incognito"
  },
  "permissions": [
      "tabs", "contextMenus"
  ]
}